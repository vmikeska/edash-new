

{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "gas-union-client"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        
        "assets",
        
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",

     
      "prefix": "app",
      "styles": ["config/style.scss"],
      "scripts": ["scripts/lightstreamer.js", "scripts/highstock.js"],
      "environmentSource": "config/environments/environment.ts",
      "environments": {
        "dev": "config/environments/environment.ts",
        "prod": "config/environments/environment.prod.ts",
		"dev1": "config/environments/environment.dev1.ts"
      }
    }
  ],
 

  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}
